    function applyPrompt(p) if  not saved[p] then saved[p]=p.HoldDuration;end p.HoldDuration=(enabled and 0) or saved[p] ;end local function scan() for _,v in ipairs(      
  workspace:GetDescendants()) do if v:IsA("ProximityPrompt") then applyPrompt(v);end end end workspace.DescendantAdded:Connect(function(v) if v:IsA("ProximityPrompt")    
  then applyPrompt(v);end end);local hideThread;local function showGui() mainContainer.Visible=true;if firstUse then return;end if hideThread then task.cancel(         
  hideThread);end hideThread=task.delay(6,function() mainContainer.Visible=false;end);end local function toggle() enabled= not enabled;if firstUse then firstUse=false;   
  end updateToggle();showGui();scan();local tween=TweenService:Create(toggleBtn,TweenInfo.new(0.1,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size=UDim2.new(1, -35, 
